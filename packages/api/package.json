{
  "name": "marvel-api",
  "version": "1.0.0",
  "description": "Marvel API for entertainment",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "nodemon --exec ./node_modules/@babel/node/bin/babel-node.js bin/www",
    "build": "npm-run-all clean transpile",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development npm-run-all build server",
    "lint": "npx eslint --fix *.js resources config bin test *.js",
    "precommit": "lint-staged",
    "transpile": "npx babel ./ -d build",
    "test": "jest",
    "watch:dev": "nodemon --exec ./node_modules/@babel/node/bin/babel-node.js bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/diguinhorocks/mapi.git"
  },
  "keywords": [
    "marvel",
    "api"
  ],
  "author": "Rodrigo Santiago Motta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diguinhorocks/mapi/issues"
  },
  "homepage": "https://github.com/diguinhorocks/mapi#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "js-md5": "^0.4.2",
    "request": "^2.80.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.4",
    "@babel/node": "^7.15.4",
    "@babel/preset-env": "^7.15.4",
    "eslint": "6.2.2",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6"
  },
  "lint-staged": {
    "**/*.js": [
      "yarn lint",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10"
          }
        }
      ]
    ]
  }
}
