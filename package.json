{
  "name": "marvel-api",
  "version": "1.0.0",
  "description": "Marvel API for entertainment",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node ./dist/api/bin/www",
    "build": "npm-run-all clean transpile",
    "dev": "NODE_ENV=development npm-run-all build server",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "watch:dev": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/diguinhorocks/mapi.git"
  },
  "keywords": [
    "marvel",
    "api"
  ],
  "author": "Rodrigo Santiago Motta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diguinhorocks/mapi/issues"
  },
  "homepage": "https://github.com/diguinhorocks/mapi#readme",
  "dependencies": {
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "js-md5": "^0.4.2",
    "request": "^2.80.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10"
          }
        }
      ]
    ]
  }
}
